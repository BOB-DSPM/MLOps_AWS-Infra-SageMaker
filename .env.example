# AWS 기본
AWS_ACCOUNT_ID=123456789012
AWS_REGION=ap-northeast-2

# 데이터/아티팩트 버킷
DATA_BUCKET=my-mlops-dev-data
ARTIFACTS_BUCKET=my-mlops-dev-artifacts

# SageMaker 실행 역할 ARN (학습/추론 컨테이너가 Assume 하는 역할)
SM_EXEC_ROLE_ARN=arn:aws:iam::123456789012:role/your-sm-exec-role

# 모델 레지스트리/엔드포인트/인스턴스 타입
PKG_GROUP_NAME=my-mlops-dev-pkg
ENDPOINT_NAME=my-mlops-dev-endpoint
SM_INSTANCE_TYPE=ml.m5.large

# (선택) 훈련 컨테이너 이미지 URI (비우면 SDK가 region 기본 XGBoost 이미지 사용)
TRAIN_IMAGE_URI=

# 파이프라인 소스 (CodeCommit 사용 예)
CODECOMMIT_REPO_NAME=my-mlops-repo
CODECOMMIT_BRANCH=main
# true면 CDK가 CodeCommit 리포를 새로 생성, false면 기존 리포를 참조
CREATE_CODECOMMIT=true

# (선택) 스케줄(CloudWatch/EventBridge Cron 표현식) – 없으면 비정기
CT_SCHEDULE_CRON=
