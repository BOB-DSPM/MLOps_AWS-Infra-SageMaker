{
    "app": "python3 app.py",
    "context": {
        "project_name": "my-mlops",
        "env_name": "dev",
        "region": "ap-northeast-2",

        "include_vpc": true,
        "enable_pipeline": true,
        "codecommit_repo_name": "my-mlops-repo",
        "pipeline_branch": "main",

        "artifact_bucket_lifecycle_days": 90,
        "ecr_untagged_keep": 20,

        "enable_sagemaker_ci": true,
        "sm_pkg_group_name": "my-mlops-dev-pkg",
        "sm_endpoint_name": "my-mlops-dev-endpoint",
        "sm_instance_type": "ml.m5.large",
        "ct_schedule_cron": "cron(0 0 * * ? *)",
        "feature_group_name": "my-mlops-dev-feature-group-ctr",
        "use_existing_feature_group": false,
        "use_sm_pipeline": true,
        "use_feature_store": true,
        "enable_studio": true,
        "studio_user": "admin"
    }
}